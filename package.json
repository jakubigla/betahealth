    "eslint": "^2.10.2",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-mocha": "^2.2.0",
    "express": "^4.13.3",
    "glob": "^6.0.4",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-eslint": "^2.0.0",
    "gulp-imagemin": "^3.0.1",
